        "scope": "source.kl"
    },
    "for": {
        "prefix": "for",
        "body": "for (var ${20:i} = ${1:Things}.length - 1; ${20:i} >= 0; ${20:i}--) {\n\t${100:${1:Things}[${20:i}]}$0\n};",
        "description": "for (…) {…} (Improved Native For-Loop)",
        "scope": "source.kl"
    },
    "for1": {
        "prefix": "for",
        "body": "for (var ${20:i} = 0; ${20:i} < ${1:Things}.length; ${20:i}++) {\n\t${100:${1:Things}[${20:i}]}$0\n};",
        "description": "for (…) {…}",
        "scope": "source.kl"
    },
    "fun": {
        "prefix": "fun",
        "body": "function ${1:function_name} (${2:argument}) {\n\t${0:// body...}\n}",
        "description": "Function",
        "scope": "source.kl"
    },
    "f": {
        "prefix": "f",
        "body": "function($1) {${0:$TM_SELECTED_TEXT}};",
