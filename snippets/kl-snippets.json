        "scope": "source.kl"
    },
    ":,": {
        "prefix": ":,",
        "body": "${1:value_name}:${0:value},",
        "description": "Object Value JS",
        "scope": "source.kl"
    },
    ":": {
        "prefix": ":",
        "body": "${1:key}: ${2:\"${3:value}\"}${4:, }",
